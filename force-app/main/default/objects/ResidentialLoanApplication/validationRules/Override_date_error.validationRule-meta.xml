<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Override_date_error</fullName>
    <active>true</active>
    <errorConditionFormula>Date_of_Override__c &gt; Today()</errorConditionFormula>
    <errorDisplayField>Date_of_Override__c</errorDisplayField>
    <errorMessage>The override date cannot be greater than today</errorMessage>
</ValidationRule>
